<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rose.tiketku">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EditProfile"></activity>
        <activity android:name=".MyTicket" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".SuccessBuyTicketActivity" />
        <activity android:name=".TicketCheckoutActivity" />
        <activity android:name=".TicketDetailActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".Register.SuccessRegisterActivity" />
        <activity android:name=".Register.RegisterTwoActivity" />
        <activity android:name=".Register.RegisterOneActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".GetStartedActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>